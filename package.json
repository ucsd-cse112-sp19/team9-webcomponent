{
  "name": "runtimeterror",
  "description": "RuntimeTerror",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint ./web-component && stylelint ./web-component",
    "docs": "jsdoc -c jsdoc.json",
    "testcafe": "testcafe chrome test/cafe-tests test/cafe-tests-mqtt",
    "browsers": "testcafe -b saucelabs",
    "saucelabs": "testcafe \"saucelabs:Chrome,saucelabs:Firefox,saucelabs:Safari\"  test/cafe-tests",
    "coverage": "codeclimate-test-reporter < ./coverage/lcov.info",
    "build": "npx webpack --config webpack.config.js",
    "start": "npx webpack-dev-server --open",
    "resume": "npx webpack-dev-server",
    "mocha": "mocha --exit -r jsdom-global/register -r document-register-element",
    "pretest": "npm run lint",
    "test": "nyc npm run mocha",
    "posttest": "npm run saucelabs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "nyc": {
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "check-coverage": true,
    "extension": [
      ".js"
    ],
    "temp-directory": ".nyc_output",
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ucsd-cse112/team9-webcomponent.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ucsd-cse112/team9-webcomponent/issues"
  },
  "homepage": "https://github.com/ucsd-cse112/team9-webcomponent#readme",
  "devDependencies": {
    "better-docs": "^1.1.6",
    "chromedriver": "^74.0.0",
    "coveralls": "^3.0.3",
    "document-register-element": "^1.13.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.12.4",
    "geckodriver": "^1.16.2",
    "husky": "^2.2.0",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "mqtt": "^3.0.0",
    "nyc": "^14.1.1",
    "puppeteer": "^1.16.0",
    "selenium-server": "^3.141.59",
    "showroom": "^0.9.3",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-standard": "^18.3.0",
    "testcafe": "^1.1.3",
    "testcafe-browser-provider-saucelabs": "^1.7.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.2",
    "ent": "^2.2.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.2",
    "jsdoc-mermaid": "^1.0.0",
    "jsdom-global": "^3.0.2",
    "mermaid": "^8.0.0",
    "mocha": "^6.1.4",
    "sauce-connect-launcher": "^1.2.6",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "socket.io": "^2.2.0",
    "webpack": "^4.31.0",
    "webpack-dev-server": "^3.3.1",
    "yargs": "^13.2.4"
  }
}
